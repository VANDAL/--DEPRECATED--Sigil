.c.o:
	if $(COMPILE) -MT $@ -MD -MP -MF "$(DEPDIR)/$*.Tpo" -c -o $@ $<; \
	then mv -f "$(DEPDIR)/$*.Tpo" "$(DEPDIR)/$*.Po"; else rm -f "$(DEPDIR)/$*.Tpo"; exit 1; fi
#	source='$<' object='$@' libtool=no \
#	DEPDIR=$(DEPDIR) $(CCDEPMODE) $(depcomp) \
#	$(COMPILE) -c $<

.c.obj:
	if $(COMPILE) -MT $@ -MD -MP -MF "$(DEPDIR)/$*.Tpo" -c -o $@ `$(CYGPATH_W) '$<'`; \
	then mv -f "$(DEPDIR)/$*.Tpo" "$(DEPDIR)/$*.Po"; else rm -f "$(DEPDIR)/$*.Tpo"; exit 1; fi
#	source='$<' object='$@' libtool=no \
#	DEPDIR=$(DEPDIR) $(CCDEPMODE) $(depcomp) \
#	$(COMPILE) -c `$(CYGPATH_W) '$<'`

exp_dhat_amd64_linux-dh_main.o: dh_main.c
	if $(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(exp_dhat_amd64_linux_CPPFLAGS) $(CPPFLAGS) $(exp_dhat_amd64_linux_CFLAGS) $(CFLAGS) -MT exp_dhat_amd64_linux-dh_main.o -MD -MP -MF "$(DEPDIR)/exp_dhat_amd64_linux-dh_main.Tpo" -c -o exp_dhat_amd64_linux-dh_main.o `test -f 'dh_main.c' || echo '$(srcdir)/'`dh_main.c; \
	then mv -f "$(DEPDIR)/exp_dhat_amd64_linux-dh_main.Tpo" "$(DEPDIR)/exp_dhat_amd64_linux-dh_main.Po"; else rm -f "$(DEPDIR)/exp_dhat_amd64_linux-dh_main.Tpo"; exit 1; fi
#	source='dh_main.c' object='exp_dhat_amd64_linux-dh_main.o' libtool=no \
#	DEPDIR=$(DEPDIR) $(CCDEPMODE) $(depcomp) \
#	$(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(exp_dhat_amd64_linux_CPPFLAGS) $(CPPFLAGS) $(exp_dhat_amd64_linux_CFLAGS) $(CFLAGS) -c -o exp_dhat_amd64_linux-dh_main.o `test -f 'dh_main.c' || echo '$(srcdir)/'`dh_main.c

